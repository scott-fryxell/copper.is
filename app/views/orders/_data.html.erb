<%-cache order do-%>
<data itemscope itemtype='order' itemid='/orders/<%=order.id%>'>
  <time itemprop="updated_at" pubdate><%=order.updated_at.getutc.iso8601%></time>
  <time itemprop="created_at"><%=order.created_at.getutc.iso8601%></time>
  <data itemprop="order_state"><%=order.order_state%></data>
  <data itemprop="charge_token"><%=order.charge_token%></data>
  <data itemprop="order_state"><%=order.order_state%></data>
  <data itemscope itemtype='user' itemid='/users/<%=order.user_id%>'></data>
  <data itemscope itemtype='tip' itemid='/orders/<%=order.id%>/tips'></data>
</data>
<%-end-%>