<%-cache page do-%>
<dl <%=page.as_item_attributes%> >
  <dd itemprop='title' contenteditable><%=page.title%></dd>
  <dd itemprop='url' contenteditable><%=page.url%></dd>
  <dd itemprop='thumbnail_url' contenteditable><%=page.thumbnail_url%></dd>
  <dd itemprop="created_at"><%=page.created_at.getutc.iso8601%></dd>
  <dd itemprop="updated_at"><%=page.updated_at.getutc.iso8601%></dd>
  <dd class="admin" itemprop='author_state'><%=page.author_state%></dd>
  <dd class="admin" itemprop='author_id'><%=page.author_id%></dd>
  <dd class="admin" itemprop='nsfw'><%=page.nsfw?%></dd>
</dl>
<%-end-%>
