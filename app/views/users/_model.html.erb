<%-cache user do-%>
<dl itemscope itemtype='users' itemid='/users/<%=user.id%>'>
  <dd itemprop="id"><%=user.id%></dd>
  <dd itemprop="created_at"><%=user.created_at.getutc.iso8601%></dd>
  <dd itemprop="updated_at"><%=user.updated_at.getutc.iso8601%></dd>
  <dd itemprop='name'><%=user.name%></dd>
  <dd itemprop="email"><%=user.email%></dd>
  <dd itemprop="tip_preference_in_cents"><%=user.tip_preference_in_cents%></dd>
  <dd itemprop="stripe_id"><%=user.stripe_id%></dd>
  <dd itemprop='accept_terms'><%=user.accept_terms%></dd>
  <dd itemprop='share_on_facebook'><%=user.share_on_facebook%></dd>
  <dd itemprop="payable_to"><%=user.payable_to%></dd>
  <dd itemprop="line1"><%=user.line1%></dd>
  <dd itemprop="line1"><%=user.line2%></dd>
  <dd itemprop="city"><%=user.city%></dd>
  <dd itemprop="subregion_code"><%=user.subregion_code%></dd>
  <dd itemprop="postal_code"><%=user.postal_code%></dd>
  <dd itemprop="country_code"><%=user.country_code%></dd>
</dl>
<%-end-%>